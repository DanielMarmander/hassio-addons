{
    "name": "RPi RF Receiver 3 - Cloned from https://github.com/pantomax/hassio-addons/",
    "version": "2.06",
    "slug": "rfreceiver3",
    "description": "RPi RF Receiver 3 Script",
    "startup": "application",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
    ],
    "boot": "auto",
    "devices": ["/dev/mem:/dev/mem:rw","/dev/gpiochip0:/dev/gpiochip0:rwm"],
    "privileged": ["SYS_RAWIO"],
    "services": [
      "mqtt:need"
    ],
    "gpio": "true",
    "apparmor": "false",
    "full_access": "true",
    "init": "false",
    "ports": {"5055/tcp": null},
    "options": {
  "code": "/share/rpi-rf_receive.py",
  "requirements": [
    "rpi-rf"
  ],
  "clean": true
},
    "schema": {"code": "str",
	       "requirements": ["str"],
	       "clean": "bool?"},
    "map": ["share"]
} 
